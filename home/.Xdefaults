! Rxvt
Rxvt*geometry:		80x35
Rxvt*foreground:	white
Rxvt*background:	black
Rxvt*cursorColor:	light goldenrod yellow
Rxvt*font:		xft:Roboto Mono Medium for Powerline:pixelsize=20
Rxvt*boldFont:		xft:Roboto Mono Medium for Powerline:pixelsize=20
Rxvt*loginShell:	on
Rxvt*visualBell:	on
Rxvt*scrollBar:		off
Rxvt*borderColor:	light goldenrod yellow

! 0=black, 1=red,  2=green,  3=yellow,	4=blue, 5=magenta,  6=cyan,  7=white
Rxvt*color0:		gray50
Rxvt*color1:		OrangeRed
Rxvt*color2:		LightGoldenrod1
Rxvt*color3:		light yellow
Rxvt*color4:		DeepSkyBlue4
Rxvt*color5:		orange
Rxvt*color6:		light cyan
Rxvt*color7:		white
Rxvt*color8:		gray50
Rxvt*color9:		OrangeRed
Rxvt*color10:		cyan1
Rxvt*color11:		light sky blue
Rxvt*color12:		DeepSkyBlue2
Rxvt*color13:		orange
Rxvt*color14:		aquamarine2
Rxvt*color15:		white

URxvt.font:		xft:Roboto Mono Medium for Powerline:pixelsize=18
URxvt*scrollTtyOutput:	false
URxvt*scrollWithBuffer:	true
URxvt*scrollTtyKeypress:	true

! Emacs
Emacs.geometry:		80x50
Emacs.foreground:	white
Emacs.background:	black
Emacs.cursorColor:	aquamarine1
Emacs.font:		Roboto Mono Medium for Powerline-18
Emacs.toolBar:		off
Emacs.menuBar:		off
Emacs.verticalScrollBars:	off
Emacs.reverseVideo:	off
